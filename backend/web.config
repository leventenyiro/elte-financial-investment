<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="src/index.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="nodejs">
          <match url="/*"/>
          <action type="Rewrite" url="src/index.js"/>
        </rule>
      </rules>
    </rewrite>
    <iisnode />
  </system.webServer>
</configuration>
